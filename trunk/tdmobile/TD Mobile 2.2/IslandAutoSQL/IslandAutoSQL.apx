  <webpages>
    <page name="EditEmployee">
      <supporteddisplays>
        <display id="phone" />
      </supporteddisplays>
      <description>Edits the details of an Employee record</description>
      <properties>
        <propertylist display="phone">
          <property type="Children" value="bgt2,df2,dfFirstName,dfLastName,cmbDeptID,dfDateHired,dfCurrentSalary,dfEmail,dfPhone,lg1" />
          <property type="FooterVisible" value="Yes" />
          <property type="Theme" value="A" />
          <property type="Authenticate" value="Yes" />
        </propertylist>
      </properties>
      <menu />
      <contents>
        <control name="__footer" type="Toolbar">
          <properties>
            <propertylist display="phone">
              <property type="Children" value="nav1" />
              <property type="Theme" value="E" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="__header" type="Toolbar">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="Employee" />
              </property>
              <property type="Children" value="pbSave,pbCancel" />
              <property type="Theme" value="Use Parent" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="dfFirstName" type="TextBox">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="First Name:" />
              </property>
              <property type="Binding" value="EMPLOYEE.sFirstName" />
              <property type="TopMargin" value="5" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="dfLastName" type="TextBox">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="Last Name:" />
              </property>
              <property type="Binding" value="EMPLOYEE.sLastName" />
              <property type="TopMargin" value="5" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="dfDateHired" type="TextBox">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="Date Hired:" />
              </property>
              <property type="Format" value="Date" />
              <property type="Binding" value="EMPLOYEE.dtDateHired" />
              <property type="TopMargin" value="5" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="pbSave" type="Button">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="__header" />
              <property type="Column" value="2" />
              <property type="TopMargin" value="0" />
              <property type="FileName" value="Check" />
              <property type="Title">
                <value culture="en-US" title="Save" />
              </property>
              <property type="Theme" value="B" />
            </propertylist>
          </properties>
          <events>
            <event name="Click">
              <actions>
                <action action="If" data="ISNEW" extradata="">
                  <action action="Alert" data="" extradata="&quot;Add ability is disabled in this demo mode. &lt;br&gt;Uncomment source code to enable it.&quot;" commented="true" />
                  <comment>! Uncomment following statement to allow adding a record</comment>
                  <action action="Invoke" data="doAddEmpolyee" extradata="" />
                </action>
                <action action="Else" data="" extradata="">
                  <action action="Alert" data="" extradata="&quot;Update ability is disabled in this demo mode. &lt;br&gt;Uncomment source code to enable it.&quot;" commented="true">
                    <action action="Navigate" data="Employees" extradata="" />
                  </action>
                  <comment>! Uncomment following statements to allow updating the record</comment>
                  <action action="Invoke" data="doSaveChanges" extradata="">
                    <action action="Navigate" data="Employees" extradata="" />
                  </action>
                </action>
              </actions>
            </event>
          </events>
        </control>
        <control name="pbCancel" type="Button">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="__header" />
              <property type="TopMargin" value="0" />
              <property type="Title">
                <value culture="en-US" title="Cancel" />
              </property>
              <property type="FileName" value="Delete" />
              <property type="Theme" value="B" />
            </propertylist>
          </properties>
          <events>
            <event name="Click">
              <actions>
                <action action="Navigate" data="&lt;back&gt;" extradata="" />
              </actions>
            </event>
          </events>
        </control>
        <control name="cmbDeptID" type="ComboBox">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="Dept. ID:" />
              </property>
              <property type="Binding" value="EMPLOYEE.sDeptID" />
              <property type="TopMargin" value="5" />
              <property type="RightMargin" value="150" />
              <property type="Theme" value="B" />
            </propertylist>
          </properties>
          <listinitialization>
            <text value="CSS" />
            <text value="FIN" />
            <text value="HR" />
            <text value="MFG" />
            <text value="MIS" />
            <text value="MKTG" />
            <text value="PD" />
          </listinitialization>
          <events />
        </control>
        <control name="dfEmail" type="TextBox">
