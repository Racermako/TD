            </propertylist>
          </properties>
          <events />
        </control>
        <control name="nav1" type="Navigator">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="__footer" />
              <property type="Children" value="pbCompanies,pbProducts,pbEmployees" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="pbCompanies" type="Button">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="nav1" />
              <property type="TopMargin" value="0" />
              <property type="Title">
                <value culture="en-US" title="Companies" />
              </property>
            </propertylist>
          </properties>
          <events>
            <event name="Click">
              <actions>
                <action action="Navigate" data="SelectCompany" extradata="" />
              </actions>
            </event>
          </events>
        </control>
        <control name="pbProducts" type="Button">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="nav1" />
              <property type="TopMargin" value="0" />
              <property type="Title">
                <value culture="en-US" title="Products" />
              </property>
            </propertylist>
          </properties>
          <events>
            <event name="Click">
              <actions>
                <action action="Navigate" data="Products" extradata="" />
              </actions>
            </event>
          </events>
        </control>
        <control name="pbEmployees" type="Button">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="nav1" />
              <property type="TopMargin" value="0" />
              <property type="Title">
                <value culture="en-US" title="Employees" />
              </property>
            </propertylist>
          </properties>
          <events>
            <event name="Click">
              <actions>
                <action action="Navigate" data="Employees" extradata="" />
              </actions>
            </event>
          </events>
        </control>
        <control name="dfPhone" type="TextBox">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="Phone:" />
              </property>
              <property type="Format" value="Tel" />
              <property type="Binding" value="EMPLOYEE.sPhone" />
              <property type="TopMargin" value="5" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="bgt2" type="Label">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="New Employee" />
              </property>
              <property type="Justify" value="Center" />
              <property type="VisibilityBinding" value="!EMPLOYEE.nID" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="df2" type="TextBox">
          <properties>
            <propertylist display="phone">
              <property type="Title">
                <value culture="en-US" title="ID" />
              </property>
              <property type="Binding" value="EMPLOYEE.nID" />
              <property type="VisibilityBinding" value="" />
              <property type="EnabledBinding" value="ISNEW" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="lg2" type="LayoutGrid">
          <properties>
            <propertylist display="phone">
              <property type="Children" value="bgt1,imgPhoto" />
              <property type="TopMargin" value="0" />
              <property type="ParentControl" value="lg1" />
            </propertylist>
          </properties>
          <events />
        </control>
      </contents>
      <bindings>
        <binding name="EMPLOYEE" type="dclsEmployee">
          <state mode="NotNull" />
          <initialvalue mode="Null" />
          <subbindings>
            <binding name="nID" type="String">
              <state mode="DoesntEqualValue" comparevalue="" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="sFirstName" type="String">
              <state mode="DoesntEqualValue" comparevalue="" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="sLastName" type="String">
              <state mode="DoesntEqualValue" comparevalue="" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="sDeptID" type="String">
              <state mode="DoesntEqualValue" comparevalue="" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="dtDateHired" type="Date/Time">
              <state mode="NotNull" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="nCurrentSalary" type="Number">
              <state mode="DoesntEqualValue" comparevalue="0" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="sEmail" type="String">
              <state mode="DoesntEqualValue" comparevalue="" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="binPhoto" type="Binary">
              <state mode="None" />
              <initialvalue mode="Null" />
            </binding>
            <binding name="sPhone" type="String">
              <state mode="DoesntEqualValue" comparevalue="" />
              <initialvalue mode="Null" />
            </binding>
          </subbindings>
        </binding>
      </bindings>
      <javascripts />
