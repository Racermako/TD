                <value culture="en-US" title="Photo:" />
              </property>
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="imgPhoto" type="Picture">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="lg2" />
              <property type="TopMargin" value="0" />
              <property type="Column" value="1" />
              <property type="Binding" value="EMPLOYEE.binPhoto" />
              <property type="CssClass" value="smallpic" />
            </propertylist>
          </properties>
          <events />
        </control>
        <control name="df1" type="TextBox">
          <properties>
            <propertylist display="phone">
              <property type="ParentControl" value="lg1" />
              <property type="TopMargin" value="0" />
              <property type="Row" value="1" />
              <property type="Binding" value="EMPLOYEE.binPhoto" />
